{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "@/components/*": [
                "./src/components/*"
            ],
            "@/lib/*": [
                "./src/lib/*"
            ],
            "@/hooks/*": [
                "./src/hooks/*"
            ],
            "@/pages/*": [
                "./src/pages/*"
            ],
            "@/entities/*": [
                "./entities/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}